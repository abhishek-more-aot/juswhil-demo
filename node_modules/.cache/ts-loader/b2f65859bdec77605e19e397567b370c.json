,CAACK,QAAQ,EAAE,CAAC,EAAEC,SAAS,CAAC;QAC/CtC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACgB,OAAO,CAAC;QACzB,IAAI,CAACsB,WAAW,EAAE;MACpB;KACD,CAAC;EACJ;EAEQ,MAAMA,WAAW;IACvB,IAAI;MACF,MAAMC,WAAW,GAAG,MAAM,oCAAW,EAAC,IAAI,CAACvB,OAAO,CAAC;MACnD,IAAI,CAACT,eAAe,CAAC,KAAK,CAAC;MAC3B;KACD,CAAC,OAAO5B,KAAK,EAAE;EAGlB;EAEQ6D,YAAY,CAAChE,IAAS;IAC5B,IAAIA,IAAI,EAAE;MACR,IAAI,CAACiE,QAAQ,CACX,IAAI,CAACC,GAAG,CAAC,0BAA0B,EAAEC,SAAS,EAAE;QAAEC,IAAI,EAAEpE,IAAI,CAACoE;MAAI,CAAE,CAAC,EACpE,IAAI,CAACF,GAAG,CAAC,cAAc,CAAC,EACxB;QACEG,iBAAiB,EAAE,IAAI,CAACH,GAAG,CAAC,cAAc,CAAC;QAC3CI,kBAAkB,EAAE,mBAAmB;QACvCC,gBAAgB,EAAE,QAAQ;QAC1BrD,IAAI,EAAE;OACP,CACF,CACEL,IAAI,CAAC,MAAK;QACT,sCAAmB,EAACb,IAAI,CAACL,EAAE,CAAC,CAACkB,IAAI,CAAC2D,OAAO,IAAG;UAC1C,IAAI,CAAC1E,OAAO,EAAE;QAChB,CAAC,CAAC;MACJ,CAAC,CAAC,CACD4B,KAAK,CAAC,MAAK;QACV,IAAI,CAACN,QAAQ,CAAC;UACZF,IAAI,EAAE,MAAM;UACZD,OAAO,EAAE,IAAI,CAACiD,GAAG,CAAC,sBAAsB;SACzC,CAAC;MACJ,CAAC,CAAC;;EAER;EAEQO,UAAU,CAACzE,IAAS;IAC1B,MAAM;MAAE0E,IAAI;MAAEC;IAAK,CAAE,GAAG3E,IAAI;IAC5B,IAAI0E,IAAI,EAAE;MACR,IAAI,CAACnF,SAAS,CAACR,IAAI,GACnB,GAAG2F,IAAI,GAAG,IAAIC,KAAK,KAAK,WAAW,GAAG,KAAK,GAAG,MAAM,CAAC;MACrD,IAAI,CAAC/C,YAAY,EAAE;;EAEvB;EAEQgD,YAAY,CAACxF,GAAW;IAC9B,MAAML,IAAI,GAAG,IAAI,CAACQ,SAAS,CAACR,IAAI;IAChC,OAAOA,IAAI,KAAK,GAAGK,GAAG,MAAM,GACxB,WAAW,GACXL,IAAI,KAAK,GAAGK,GAAG,OAAO,GACpB,YAAY,GACZ,EAAE;EACV;EAEQyF,cAAc;IACpB,IAAI,CAACC,eAAe,GAAG,IAAI;IAC3B,MAAMC,OAAO,GAAG,CACd,IAAI,EACJ,WAAW,EACX,OAAO,EACP,gBAAgB,EAChB,QAAQ,EACR,WAAW,CACZ;IACD,MAAMC,SAAS,GAAG,CAChB,IAAI,EACJ,UAAU,EACV,OAAO,EACP,eAAe,EACf,SAAS,EACT,kBAAkB,CACnB;IACD,MAAMhF,IAAI,GAAG,qBAAU,EAACgF,SAAS,EAAE,IAAI,CAAC/E,IAAI,CAAC;IAC7C,2BAAgB,EAAC8E,OAAO,EAAE/E,IAAI,EAAE,IAAI,CAACkE,GAAG,CAAC,2BAA2B,CAAC,CAAC;IACtE,IAAI,CAACY,eAAe,GAAG,KAAK;EAC9B;CACH;AAzLC,wBADGG,4BAAM,yCA4BR;AAvGHC,oCAPC,mCAAS,EAAC;EACTd,IAAI,EAAE,UAAU;EAChBe,UAAU,EAAE;IACVC,UAAU,EAAVA,cAAU;IACVC,OAAO,EAAPA;;CAEH,CAAC,aAsQD;AAAA","names":["Vue","constructor","Object","assign","defaultProductOptionData","AdminModule","roles","page","limit","sort","productId","enabled","filter","label","key","value","created","listQuery","parseInt","$route","params","id","postForm","fetchOption","getList","listLoading","data","list","total","error","response","optionList","options","err","Promise","reject","saveForm","loading","optionId","then","res","$notify","title","message","type","duration","$message","updateIsSelected","isSelectedLoading","console","log","dataModel","catch","e","handleFilter","onPriortyChange","showSortingOption","fetchAllRecored","renderComponent","sortable","disabled","$nextTick","checkForFirstTime","oldList","map","v","newList","slice","setSort","temp","$refs","dragTable","el","$el","querySelectorAll","Sortable","create","ghostClass","setData","dataTransfer","onEnd","evt","targetRow","splice","Number","oldIndex","newIndex","tempIndex","updateOrder","updateCheck","handleDelete","$confirm","$tc","undefined","name","confirmButtonText","confirmButtonClass","cancelButtonText","deleted","sortChange","prop","order","getSortClass","handleDownload","downloadLoading","tHeader","filterVal","Action","default_1","components","Pagination","Enabled"],"sourceRoot":"","sources":["/home/algoocean/Desktop/AOT-Project/myvarni-admin/node_modules/cache-loader/dist/cjs.js??ref--1-0!/home/algoocean/Desktop/AOT-Project/myvarni-admin/node_modules/vue-loader/lib/index.js??vue-loader-options!/home/algoocean/Desktop/AOT-Project/myvarni-admin/src/views/product/components/OptionSelect.vue?vue&type=script&lang=ts&"],"sourcesContent":["\nimport { Component, Vue } from 'vue-property-decorator'\nimport { IProductData } from '@/api/types'\nimport Pagination from '@/components/Pagination/index.vue'\nimport { exportJson2Excel } from '@/utils/excel'\nimport { formatJson, getQuery } from '@/utils'\nimport Enabled from '@/components/Enabled/index.vue'\nimport { AdminModule } from '@/store/modules/admin'\nimport Sortable from 'sortablejs'\nimport { Action } from 'vuex-module-decorators'\nimport { addProductOption, defaultProductOptionData, deleteProductOption, getProductOptions, updateProductOption, updateProductOptionIsSelected } from '@/api/productOption'\nimport { getOptionValues } from '@/api/optionValues'\nimport { updateOrder } from '@/api/productOptionValues'\nimport { getOptions } from '@/api/option'\nimport { availableOptions } from '@/api/product'\n\n@Component({\n  name: 'TypeList',\n  components: {\n    Pagination,\n    Enabled\n  }\n})\nexport default class extends Vue {\n  private tableKey = 0;\n  private total = 0;\n  optionList:any = []\n  private loading = false;\n  private postForm = Object.assign({}, defaultProductOptionData);\n  private list: IProductData[] = [];\n  isSelectedLoading: boolean=false;\n  private myRole = AdminModule.roles;\n  private listLoading = true;\n  newList: any = [];\n  oldList: any = [];\n  private showSortingOption: boolean = false;\n  private renderComponent: boolean = true;\n  sortable: any;\n  private downloadLoading = false;\n  private listQuery = {\n    page: 1,\n    limit: 20,\n    sort: 'id,DESC',\n    productId: 0,\n    enabled:true,\n    'option.name': '',\n    filter: {\n      enabled: 'eq',\n      'option.name': '$contL',\n      productId: 'eq'\n    }\n  };\n\n  private showCreatedDate: boolean = false;\n  private sortOptions = [\n    { label: 'Ascending', key: 'id,ASC' },\n    { label: 'Descending', key: 'id,DESC' }\n  ];\n\n  private enableTypeOptions = [\n    { key: true, value: 'Enable' },\n    { key: false, value: 'Disabled' }\n  ];\n\n  private filters: any = [];\n\n  created() {\n    this.listQuery.productId = parseInt(this.$route.params && this.$route.params.id)\n    this.postForm.productId = this.listQuery.productId\n    this.fetchOption()\n    this.getList()\n  }\n\n  private async getList() {\n    try {\n      this.listLoading = true\n      const data: any = await getProductOptions(getQuery(this.listQuery))\n      this.list = data.data\n      this.total = data.total\n      this.listLoading = false\n      this.fetchOption()\n    } catch (error) {\n      this.listLoading = false\n    }\n  }\n\n  private async fetchOption() {\n    const data = {\n      productId: this.listQuery.productId\n    }\n    try {\n      const response:any = await availableOptions(data)\n      this.optionList = response.options\n    } catch (err) {\n      Promise.reject(err)\n    }\n  }\n\n    @Action\n  public async saveForm() {\n    try {\n      this.loading = true\n\n      // if (this.isEdit) {\n      //   await updateOption(this.postForm.id, this.postForm)\n      // } else {\n\n      if (this.postForm.optionId) {\n        await addProductOption(this.postForm).then((res:any) => {\n          this.$notify({\n            title: 'Success',\n            message: 'Option saved successfully',\n            type: 'success',\n            duration: 2000\n          })\n          this.postForm.optionId = null\n          this.getList()\n        })\n      } else {\n        this.$message.error('Please select an option value')\n      }\n\n      // }\n    } catch (err) {\n      this.loading = false\n    }\n  }\n  async updateIsSelected(data:any){\n    this.isSelectedLoading = true;\n    console.log(data);\n    const dataModel:any ={\n      productId:data.productId,\n      id:data.id\n\n    }\n    await updateProductOptionIsSelected(dataModel).then((res:any)=>{\n      this.getList();\n    this.$message({\n          message: 'Product options updated succesfully.',\n          type: 'success'\n        });\n\n\n    }).catch((e:any)=>{\n    this.isSelectedLoading = true;\n\n\n    })\n    \n\n  }\n\n    private handleFilter() {\n      this.listQuery.page = 1\n      this.getList()\n    }\n\n    async onPriortyChange() {\n      if (this.showSortingOption) {\n        this.fetchAllRecored(true)\n      } else {\n        this.renderComponent = false\n        this.sortable.options.disabled = true\n        this.$nextTick().then(() => {\n          this.renderComponent = true\n        })\n      }\n    }\n\n    async fetchAllRecored(checkForFirstTime: boolean) {\n      try {\n        this.listLoading = true\n        this.listQuery.limit = 10000\n        const data: any = await getOptionValues(getQuery(this.listQuery))\n        this.list = data.data\n        this.total = data.total\n        this.listLoading = false\n        if (checkForFirstTime) {\n          this.oldList = this.list.map((v) => v.id)\n          this.newList = this.oldList.slice()\n          this.$nextTick(this.setSort)\n        }\n      } catch (error) {\n        this.listLoading = false\n      }\n    }\n\n    setSort() {\n      const temp: any = this.$refs.dragTable\n      const el = temp.$el.querySelectorAll(\n        '.el-table__body-wrapper > table > tbody'\n      )[0]\n      this.sortable = Sortable.create(el, {\n        ghostClass: 'sortable-ghost',\n        setData: function(dataTransfer) {\n          dataTransfer.setData('Text', '')\n        },\n        onEnd: (evt) => {\n          const targetRow = this.list.splice(Number(evt.oldIndex), 1)[0]\n          this.list.splice(Number(evt.newIndex), 0, targetRow)\n          const tempIndex = this.newList.splice(evt.oldIndex, 1)[0]\n          this.newList.splice(evt.newIndex, 0, tempIndex)\n          console.log(this.newList)\n          this.updateOrder()\n        }\n      })\n    }\n\n    private async updateOrder() {\n      try {\n        const updateCheck = await updateOrder(this.newList)\n        this.fetchAllRecored(false)\n        // eslint-disable-next-line no-empty\n      } catch (error) {\n\n      }\n    }\n\n    private handleDelete(data: any) {\n      if (data) {\n        this.$confirm(\n          this.$tc('table.deleteConfirmation', undefined, { name: data.name }),\n          this.$tc('table.delete'),\n          {\n            confirmButtonText: this.$tc('table.delete'),\n            confirmButtonClass: 'el-button--danger',\n            cancelButtonText: 'Cancel',\n            type: 'error'\n          }\n        )\n          .then(() => {\n            deleteProductOption(data.id).then(deleted => {\n              this.getList()\n            })\n          })\n          .catch(() => {\n            this.$message({\n              type: 'info',\n              message: this.$tc('table.deleteCanceled')\n            })\n          })\n      }\n    }\n\n    private sortChange(data: any) {\n      const { prop, order } = data\n      if (prop) {\n        this.listQuery.sort =\n        `${prop},` + (order === 'ascending' ? 'ASC' : 'DESC')\n        this.handleFilter()\n      }\n    }\n\n    private getSortClass(key: string) {\n      const sort = this.listQuery.sort\n      return sort === `${key},ASC`\n        ? 'ascending'\n        : sort === `${key},DESC`\n          ? 'descending'\n          : ''\n    }\n\n    private handleDownload() {\n      this.downloadLoading = true\n      const tHeader = [\n        'ID',\n        'Full Name',\n        'Email',\n        'Contact Number',\n        'Status',\n        'timestamp'\n      ]\n      const filterVal = [\n        'id',\n        'fullName',\n        'email',\n        'contactNumber',\n        'enabled',\n        'createdTimestamp'\n      ]\n      const data = formatJson(filterVal, this.list)\n      exportJson2Excel(tHeader, data, this.$tc('route.notifications.title'))\n      this.downloadLoading = false\n    }\n}\n"]}]}